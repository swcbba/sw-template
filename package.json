{
  "name": "sw-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build --prod=false",
    "build:prod": "run-s build:client build:prerender",
    "build:client": "ng build --prod --build-optimizer",
    "build:prerender": "node prerender.js",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "keys:create": "cat ./src/environments/keys.template.ts > ./src/environments/keys.ts",
    "keys:inject": "echo $KEYS > ./src/environments/keys.ts",
    "deploy": "firebase deploy -P smartcities"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.4",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.2.4",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "firebase": "^6.3.5",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "^2.24.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/materialize-css": "^1.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mz": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "puppeteer": "^1.19.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
